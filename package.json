{
  "name": "libpg-query",
  "version": "13.3.0",
  "description": "The real PostgreSQL query parser",
  "homepage": "https://github.com/pyramation/libpg-query-node",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "configure": "node-pre-gyp configure",
    "install": "node-pre-gyp install --fallback-to-build",
    "rebuild": "node-pre-gyp configure rebuild",
    "test": "mocha --timeout 5000",
    "binary:build": "node-pre-gyp rebuild package",
    "binary:publish": "node-pre-gyp publish"
  },
  "author": "Dan Lynch <pyramation@gmail.com> (http://github.com/pyramation)",
  "license": "LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git://github.com/pyramation/libpg-query-node.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.1518.0",
    "chai": "^3.5.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.8",
    "node-addon-api": "^1.6.3",
    "node-gyp": "^10.0.1"
  },
  "keywords": [
    "sql",
    "postgres",
    "postgresql",
    "pg",
    "query",
    "plpgsql",
    "database"
  ],
  "binary": {
    "module_name": "queryparser",
    "module_path": "./build/Release/",
    "host": "https://gadget-node-artifacts.s3-us-east-2.amazonaws.com",
    "remote_path": "./libpg-query-node/"
  }
}
